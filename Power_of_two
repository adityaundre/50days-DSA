function powerOfTwo(n) {
    if(n<1){
        return false
    }
    while(n>1){
        if(n%2 !=0) {   //if num%2===0 it is power of 2 else not
            return false
        }
        n=n/2         //n=4, n=4/2=2, n=2/2=0
                    //if remainder is 0 and at the end "n" comes down to 1. It is power of two
    }
    return true
}
